# ============================================
# Fochs (OpenClaw) - Environment Configuration
# Copy to .env and fill in your values
# ============================================

# --- LLM (API) ---
FOCHS_ANTHROPIC_API_KEY=sk-ant-...
FOCHS_GEMINI_API_KEY=AI...
FOCHS_XAI_API_KEY=xai-...

# --- LLM (Lokal - Ollama) ---
FOCHS_OLLAMA_HOST=http://localhost:11434
FOCHS_OLLAMA_DEFAULT_MODEL=llama3.1:8b
FOCHS_OLLAMA_FAST_MODEL=llama3.2:3b
FOCHS_OLLAMA_EMBED_MODEL=nomic-embed-text

# --- Search ---
FOCHS_BRAVE_API_KEY=BSA...

# --- Telegram ---
FOCHS_TELEGRAM_BOT_TOKEN=123456:ABC-DEF...
FOCHS_TELEGRAM_ALLOWED_USERS=[]

# --- GitHub ---
FOCHS_GITHUB_TOKEN=ghp_...

# --- E-Mail ---
FOCHS_EMAIL_ADDRESS=fochs@example.com
FOCHS_EMAIL_IMAP_HOST=imap.gmail.com
FOCHS_EMAIL_SMTP_HOST=smtp.gmail.com
FOCHS_EMAIL_PASSWORD=...

# --- Google Workspace ---
FOCHS_GOOGLE_CREDENTIALS_PATH=./credentials.json

# --- Dashboard ---
FOCHS_WEB_HOST=0.0.0.0
FOCHS_WEB_PORT=8080
FOCHS_WEB_SECRET_KEY=change-me-to-a-random-string

# --- Agent ---
FOCHS_AUTONOMY_LEVEL=ask
FOCHS_MAX_ITERATIONS=10
FOCHS_DATA_DIR=./data
